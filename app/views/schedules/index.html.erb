<h1>Schedules</h1>

<table class="responsive nowrap table table-hover" id="dttb-schedules" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="start">Start</th>
      <th data-data="end">End</th>
      <th data-data="equipament">Equipament</th>
      <th data-data="laboratory">Laboratory</th>
      <th data-data="user">User</th>
      <th data-data="status">Status</th>
      <th data-data="typeReservation">Typereservation</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

